//==============================================================================
//
//  ctime
//
#ifndef CTIME_INCLUDED
#define CTIME_INCLUDED

#include "cstdint"

typedef long clock_t;

constexpr clock_t CLOCKS_PER_SEC = 1000;

clock_t clock();

struct tm
{
   int tm_sec;
   int tm_min;
   int tm_hour;
   int tm_mday;
   int tm_mon;
   int tm_year;
   int tm_wday;
   int tm_yday;
   int tm_isdst;
};

typedef int64_t time_t;

time_t time(time_t* Time);

#endif
