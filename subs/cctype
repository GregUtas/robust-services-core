#ifndef CCTYPE_INCLUDED
#define CCTYPE_INCLUDED

bool isalpha(char c);
bool isdigit(char c);
bool isprint(char c);
bool ispunct(char c);
bool isspace(char c);
bool isxdigit(char c);
char tolower(char c);
char toupper(char c);

#endif